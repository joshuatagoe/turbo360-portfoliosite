                                        <li><a href="#">1</a></li>
                                        <li><a href="#" class="active">2</a></li>
                                        <li><a href="#">3</a></li>
                                        <li><a href="#"><i class="fa fa-caret-right"></i></a></li>
                                        <li><a href="#"><i class="fa fa-caret-right"></i><i class="fa fa-caret-right"></i></a></li>
